// Адреса устройств на шине I2C

#define DEVICE1 = 0x08h         ;Адрес 1-й Меga8
#define DEVICE1 = 0x09h         ;Адрес 2-й Меga8
#define DEVICE1 = 0x0ah         ;Адрес 3-й Меga8
#define DEVICE1 = 0x0bh         ;Адрес 4-й Меga8

//Cветодиод
#define LedRed ()  {Porta.0 = 1; Porta.1 = 0;} //Красный цвет
